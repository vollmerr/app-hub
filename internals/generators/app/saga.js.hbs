import { takeLatest, call, put } from 'redux-saga/effects';

import * as api from '../../utils/api';

import * as actions from './actions';
import * as C from './constants';


export function* getInitData() {
  try {
    const data = yield call(api.requestWithToken, 'URL GOES HERE');
    yield put(actions.getInitDataSuccess(data));
  } catch (error) {
    yield put(actions.getInitDataFailure(error));
  }
}


export default function* {{ camelCase name }}Saga() {
  yield [
    takeLatest(C.GET_INIT_DATA_REQUEST, getInitData),
  ];
}
